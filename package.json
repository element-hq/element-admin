{
  "name": "element-admin-console",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "start": "vite --port 3000",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint --max-warnings 0 . && prettier --check . && tsc",
    "fix": "eslint --fix . && prettier --write .",
    "i18n:extract": "formatjs extract --throws --format crowdin src/**/*.tsx --out-file translations/extracted/en.json && prettier --write translations/extracted/en.json",
    "i18n:download": "localazy download && prettier --write translations/translated/",
    "i18n:compile": "formatjs compile-folder --format crowdin --ast translations/translated/ translations/compiled/ && prettier --write translations/compiled",
    "prepare": "husky"
  },
  "dependencies": {
    "@fontsource/inconsolata": "5.2.6",
    "@fontsource/inter": "5.2.6",
    "@formatjs/intl-pluralrules": "5.4.4",
    "@formatjs/intl-relativetimeformat": "11.4.11",
    "@js-temporal/polyfill": "0.5.1",
    "@tailwindcss/vite": "4.1.12",
    "@tanstack/react-query": "5.85.5",
    "@tanstack/react-query-devtools": "5.85.5",
    "@tanstack/react-router": "1.131.27",
    "@tanstack/react-router-devtools": "1.131.27",
    "@tanstack/router-plugin": "1.131.27",
    "@vector-im/compound-design-tokens": "5.0.0",
    "@vector-im/compound-web": "8.2.0",
    "classnames": "2.5.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-intl": "7.1.11",
    "use-broadcast-ts": "2.0.1",
    "valibot": "1.1.0",
    "zustand": "5.0.8"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.11.6",
    "@eslint/js": "9.33.0",
    "@formatjs/cli": "6.7.2",
    "@localazy/cli": "2.0.8",
    "@tsconfig/strictest": "2.0.5",
    "@tsconfig/vite-react": "7.0.0",
    "@types/node": "24.3.0",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "@vitejs/plugin-react": "5.0.1",
    "babel-plugin-formatjs": "10.5.39",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "eslint-plugin-unicorn": "60.0.0",
    "husky": "9.1.7",
    "knip": "5.62.0",
    "lint-staged": "16.1.5",
    "prettier": "3.6.2",
    "tailwindcss": "4.1.12",
    "typescript": "5.9.2",
    "typescript-eslint": "8.40.0",
    "vite": "7.1.3",
    "vite-tsconfig-paths": "5.1.4",
    "wrangler": "4.31.0"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix --max-warnings 0"
    ]
  },
  "knip": {
    "ignore": [
      "src/routeTree.gen.ts",
      "src/api/*"
    ],
    "ignoreDependencies": [
      "tailwindcss"
    ]
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad"
}
