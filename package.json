    "@tsconfig/vite-react": "7.0.1",
    "@types/mdast": "4.0.4",
    "@types/node": "24.5.0",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "@types/semver": "7.7.1",
    "@vitejs/plugin-react": "5.0.2",
    "babel-plugin-formatjs": "10.5.39",
