    "eslint-plugin-unicorn": "61.0.2",
    "husky": "9.1.7",
    "knip": "5.63.1",
    "lint-staged": "16.1.6",
    "prettier": "3.6.2",
    "stylelint": "16.24.0",
    "stylelint-config-standard": "39.0.0",
    "stylelint-config-tailwindcss": "1.0.0",
    "tailwindcss": "4.1.13",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.0",
    "vite": "7.1.5",
    "vite-tsconfig-paths": "5.1.4",
