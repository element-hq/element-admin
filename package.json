{
  "name": "element-admin",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/element-hq/element-admin.git"
  },
  "bugs": {
    "url": "https://github.com/element-hq/element-admin/issues"
  },
  "license": "SEE LICENSE IN README.md",
  "scripts": {
    "dev": "vite --port 3000",
    "start": "vite --port 3000",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint --max-warnings 0 . && stylelint 'src/**/*.css' && prettier --check . && tsc",
    "fix": "eslint --fix . && stylelint --fix 'src/**/*.css' && prettier --write .",
    "i18n:extract": "formatjs extract --throws --format crowdin 'src/**/*.{ts,tsx}' --out-file translations/extracted/en.json && prettier --write translations/extracted/en.json",
    "i18n:download": "localazy download && prettier --write translations/translated/",
    "i18n:compile": "formatjs compile-folder --format crowdin --ast translations/translated/ translations/compiled/ && prettier --write translations/compiled",
    "openapi-ts": "openapi-ts",
    "prepare": "husky",
    "browserslist": "browserslist",
    "test": "playwright test"
  },
  "dependencies": {
    "@fontsource/inconsolata": "5.2.8",
    "@fontsource/inter": "5.2.8",
    "@formatjs/intl-localematcher": "0.6.1",
    "@js-temporal/polyfill": "0.5.1",
    "@radix-ui/react-dialog": "1.1.15",
    "@tailwindcss/vite": "4.1.13",
    "@tanstack/react-devtools": "0.7.0",
    "@tanstack/react-pacer": "0.16.3",
    "@tanstack/react-pacer-devtools": "0.3.0",
    "@tanstack/react-query": "5.90.2",
    "@tanstack/react-query-devtools": "5.90.2",
    "@tanstack/react-router": "1.131.50",
    "@tanstack/react-router-devtools": "1.131.50",
    "@tanstack/react-table": "8.21.3",
    "@tanstack/react-virtual": "3.13.12",
    "@tanstack/router-plugin": "1.131.50",
    "@vector-im/compound-design-tokens": "6.0.0",
    "@vector-im/compound-web": "8.2.4",
    "classnames": "2.5.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-error-boundary": "6.0.0",
    "react-hot-toast": "2.6.0",
    "react-intl": "7.1.11",
    "semver": "7.7.2",
    "use-broadcast-ts": "2.0.1",
    "valibot": "1.1.0",
    "vaul": "1.1.2",
    "zustand": "5.0.8"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.10.2",
    "@eslint/js": "9.36.0",
    "@formatjs/cli": "6.7.2",
    "@hey-api/openapi-ts": "0.84.1",
    "@localazy/cli": "2.0.8",
    "@playwright/test": "1.55.0",
    "@tanstack/devtools-vite": "0.3.3",
    "@tanstack/eslint-plugin-query": "5.90.1",
    "@tanstack/eslint-plugin-router": "1.132.0",
    "@tsconfig/strictest": "2.0.6",
    "@tsconfig/vite-react": "7.0.1",
    "@types/node": "24.5.2",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "@types/semver": "7.7.1",
    "@vitejs/plugin-react": "5.0.3",
    "babel-plugin-formatjs": "10.5.39",
    "babel-plugin-react-compiler": "19.1.0-rc.3",
    "browserslist": "4.26.2",
    "browserslist-to-esbuild": "2.1.1",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-compat": "6.0.2",
    "eslint-plugin-formatjs": "5.4.0",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.21",
    "eslint-plugin-unicorn": "61.0.2",
    "husky": "9.1.7",
    "knip": "5.64.0",
    "lint-staged": "16.2.0",
    "prettier": "3.6.2",
    "stylelint": "16.24.0",
    "stylelint-config-standard": "39.0.0",
    "stylelint-config-tailwindcss": "1.0.0",
    "tailwindcss": "4.1.13",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.1",
    "vite": "7.1.7",
    "vite-tsconfig-paths": "5.1.4"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix --max-warnings 0"
    ],
    "*.css": [
      "stylelint --fix"
    ]
  },
  "knip": {
    "entry": [
      "src/main.tsx",
      "src/prerender.tsx"
    ],
    "ignore": [
      "src/routeTree.gen.ts",
      "src/api/mas/api/**/*",
      "src/api/synapse.ts",
      "openapi-ts.config.ts"
    ]
  },
  "packageManager": "pnpm@10.17.0+sha512.fce8a3dd29a4ed2ec566fb53efbb04d8c44a0f05bc6f24a73046910fb9c3ce7afa35a0980500668fa3573345bd644644fa98338fa168235c80f4aa17aa17fbef"
}
